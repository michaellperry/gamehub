{
    "name": "gamehub-player",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:container": "vite build --mode container && mkdir -p ../../mesh/nginx/app/gamehub-player && cp -r dist/* ../../mesh/nginx/app/gamehub-player/",
        "build:model": "cd ../gamehub-model && npm run build",
        "generate-policies": "npm run generate-policies --workspace=gamehub-model",
        "lint": "eslint .",
        "preview": "vite preview",
        "type-check": "tsc --noEmit",
        "test": "vitest run",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest --watch"
    },
    "dependencies": {
        "gamehub-model": "file:../gamehub-model",
        "jinaga": "^6.7.8",
        "jinaga-react": "^5.2.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-oauth2-code-pkce": "^1.23.1",
        "react-router-dom": "^7.6.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^14.2.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/node": "^24.0.15",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/ui": "^1.3.1",
        "autoprefixer": "^10.4.21",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "globals": "^16.3.0",
        "jsdom": "^24.0.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.17",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.35.1",
        "vite": "^5.2.0",
        "vitest": "^1.3.1"
    }
}